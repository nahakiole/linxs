{
  "manifest_version": 3,
  "name": "X/Twitter Title Enabler",
  "version": "1.0",
  "description": "A Chrome extension that adds Titles to Links again to Twitter/X",
  "permissions": ["activeTab", "https://twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["styles/default.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
